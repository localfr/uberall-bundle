{
    "name": "localfr/uberall-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony Bundle for uberall",
    "homepage": "https://github.com/localfr/uberall-bundle",
    "keywords": ["uberall", "symfony", "bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "local.fr",
            "email": "developpeurs-web@local.fr"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^6.4",
        "symfony/http-client": "^6.4",
        "symfony/monolog-bundle": "^3.7",
        "symfony/validator": "^6.4",
        "doctrine/common": "^3.2",
        "symfony/serializer": "^6.4",
        "symfony/property-access": "^6.4",
        "doctrine/annotations": "^1.13"
    },
    "autoload": {
        "psr-4": {
            "Localfr\\UberallBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Localfr\\UberallBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5",
        "symfony/yaml": "^6.4"
    }
}
